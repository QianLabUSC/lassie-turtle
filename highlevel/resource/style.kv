<extrudetab>
    MDCard:
        size_hint_x: 1
        orientation: "vertical" 
        adaptive_height: True
        elevation: 1
        radius: [4, ]
        padding: 12
        height: 10   
        MDBoxLayout:
            padding: 30
            MDBoxLayout:
                MDLabel:
                    font_style: "Subtitle1"
                    theme_text_color: "Custom"
                    font_style: "Subtitle1"
                    text_color:  color_black
                    text: "Extrude Angle: "
                    height: self.texture_size[1]
                MDBoxLayout:
                    MDLabel:
                        id: extrude_angle
                        font_style: "Subtitle1"
                        theme_text_color: "Custom"
                        font_style: "Subtitle1"
                        text_color:  color_black
                        text: "0"
                        height: self.texture_size[1]
                    MDLabel:
                        font_style: "Subtitle1"
                        theme_text_color: "Custom"
                        font_style: "Subtitle1"
                        text_color:  color_black
                        text: "(C)"
                        height: self.texture_size[1]
            MDBoxLayout:
                size_hint_x: 0.7
        MDSlider:
            id: extrude_angle_slider
            min: -60
            max: 60
            value: 0
            hint: False
            color: color_deep_orange
            on_active: app.on_change_extrude_angle()


        MDBoxLayout:
            padding: [30, 40, 0, 0]
            MDBoxLayout:
                MDLabel:
                    font_style: "Subtitle1"
                    theme_text_color: "Custom"
                    font_style: "Subtitle1"
                    text_color:  color_black
                    text: "Extrude Speed: "
                    height: self.texture_size[1]
                MDBoxLayout:
                    MDLabel:
                        id: extrude_speed
                        font_style: "Subtitle1"
                        theme_text_color: "Custom"
                        font_style: "Subtitle1"
                        text_color:  color_black
                        text: "1"
                        height: self.texture_size[1]
                    MDLabel:
                        font_style: "Subtitle1"
                        theme_text_color: "Custom"
                        font_style: "Subtitle1"
                        text_color:  color_black
                        text: "(cm/s)"
                        height: self.texture_size[1]
            MDBoxLayout:
                size_hint_x: 0.7
                MDIconButton:
                    id: extrude_speed_plus
                    icon: "plus"
                    icon_size: "16sp"
                    on_press: app.on_plus_speed_click('extrude_speed', 1)
                MDIconButton:
                    id: extrude_speed_minus
                    icon: "minus"
                    icon_size: "16sp"
                    on_press: app.on_plus_speed_click('extrude_speed', -1)
        MDSlider:
            id: extrude_speed_slider
            min: 1
            max: 500
            value: 1
            hint: False
            color: color_deep_orange
            on_active: app.on_change_extrude_speed()

        MDBoxLayout:
            padding: [30, 40, 0, 0]
            MDBoxLayout:
                MDLabel:
                    font_style: "Subtitle1"
                    theme_text_color: "Custom"
                    font_style: "Subtitle1"
                    text_color:  color_black
                    text: "Back Speed: "
                    height: self.texture_size[1]
                MDBoxLayout:
                    MDLabel:
                        id: back_speed
                        font_style: "Subtitle1"
                        theme_text_color: "Custom"
                        font_style: "Subtitle1"
                        text_color:  color_black
                        text: "1"
                        height: self.texture_size[1]
                    MDLabel:
                        font_style: "Subtitle1"
                        theme_text_color: "Custom"
                        font_style: "Subtitle1"
                        text_color:  color_black
                        text: "(cm/s)"
                        height: self.texture_size[1]
            MDBoxLayout:
                size_hint_x: 0.7
                MDIconButton:
                    id: back_speed_plus
                    icon: "plus"
                    icon_size: "16sp"
                    on_press: app.on_plus_speed_click('back_speed', 1)
                MDIconButton:
                    id: back_speed_minus
                    icon: "minus"
                    icon_size: "16sp"
                    on_press: app.on_plus_speed_click('back_speed', -1)
        MDSlider:
            id: back_speed_slider
            min: 1
            max: 500
            value: 1
            hint: False
            color: color_deep_orange
            on_active: app.on_change_back_speed()



        
        
        MDBoxLayout:
            padding: 30
            MDBoxLayout:
                MDLabel:
                    font_style: "Subtitle1"
                    theme_text_color: "Custom"
                    font_style: "Subtitle1"
                    text_color:  color_black
                    text: "Extrude length: "
                    height: self.texture_size[1]
                MDBoxLayout:
                    MDLabel:
                        id: extrude_length
                        font_style: "Subtitle1"
                        theme_text_color: "Custom"
                        font_style: "Subtitle1"
                        text_color:  color_black
                        text: "4"
                        height: self.texture_size[1]
                    MDLabel:
                        font_style: "Subtitle1"
                        theme_text_color: "Custom"
                        font_style: "Subtitle1"
                        text_color:  color_black
                        text: "(cm)"
                        height: self.texture_size[1]
